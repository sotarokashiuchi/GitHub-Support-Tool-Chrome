{
  "manifest_version": 3,
  "name": "GitHub Support Tools",
  "version": "1.1",
  "description": "Transfer GitHub Account Name to Full Name.",
  "action": {
      "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"], 
      "js": ["content.js"]
    }
  ]
}