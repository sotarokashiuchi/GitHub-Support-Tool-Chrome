{
    "manifest_version": 3,
    "name": "Text Replacement",
    "version": "1.0",
    "description": "Replace specific text on a specific website.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://github.com/*"], 
        "js": ["content.js"]
      }
    ]
  }
  